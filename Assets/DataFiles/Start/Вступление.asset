  backGround: {fileID: 21300000, guid: 0791b6b21fad4f9449657ef95ceff9d5, type: 3}
  placeText: "\u0414\u0435\u0440\u0435\u0432\u043D\u044F \u0437\u0435\u043C\u043B\u0435\u0434\u0435\u043B\u044C\u0446\u0435\u0432"
  episodeText: "\u0412\u0441\u0442\u0443\u043F\u043B\u0435\u043D\u0438\u0435"
  contentText: "\u0412 \u043E\u0434\u0438\u043D \u043F\u0440\u0435\u043A\u0440\u0430\u0441\u043D\u044B\u0439
    \u0434\u0435\u043D\u044C, \u0432 \u043E\u0434\u043D\u043E\u0439 \u0438\u0437
    \u0434\u0435\u0440\u0435\u0432\u0435\u043D\u044C \u0432\u0435\u043B\u0438\u043A\u0438\u0445
    \u0437\u0435\u043C\u0435\u043B\u044C \u0410\u043B\u0435\u043D\u0434\u043E\u0440\u0430
    \u043E\u0431\u044B\u0447\u043D\u0430\u044F \u0441\u0435\u043C\u044C\u044F \u0437\u0435\u043C\u043B\u0435\u0434\u0435\u043B\u044C\u0446\u0435\u0432
    \u0440\u0430\u0434\u043E\u0432\u0430\u043B\u0430\u0441\u044C \u043F\u0440\u0435\u043A\u0440\u0430\u0441\u043D\u043E\u043C\u0443
    \u0441\u043E\u0431\u044B\u0442\u0438\u044E - \u0440\u043E\u0436\u0434\u0435\u043D\u0438\u044E
    \u0440\u0435\u0431\u0435\u043D\u043A\u0430. \u0421\u0447\u0430\u0441\u0442\u043B\u0438\u0432\u0430\u044F
    \u043C\u043E\u043B\u043E\u0434\u0430\u044F \u0441\u0435\u043C\u044C\u044F \u043D\u0430\u0440\u0435\u043A\u043B\u0430
    \u043C\u0430\u043B\u044B\u0448\u0430 \u041A\u0430\u043B\u0435\u0431\u043E\u043C.
    \u0412\u043E\u043B\u0448\u0435\u0431\u043D\u043E\u0435 \u0434\u0435\u0442\u0441\u0442\u0432\u043E
    \u043F\u0440\u043E\u0442\u0435\u043A\u0430\u043B\u043E \u0443 \u043C\u0430\u043B\u044C\u0447\u0438\u043A\u0430.
    \u0420\u043E\u0434\u0438\u0442\u0435\u043B\u0438 \u043F\u043E\u043A\u0430\u0437\u044B\u0432\u0430\u043B\u0438
    \u0441\u0430\u043C\u044B\u0435 \u043F\u0440\u0435\u043A\u0440\u0430\u0441\u043D\u044B\u0435
    \u043C\u043E\u043C\u0435\u043D\u0442\u044B \u0436\u0438\u0437\u043D\u0438. \u041D\u043E,
    \u043A \u0441\u043E\u0436\u0430\u043B\u0435\u043D\u0438\u044E, \u0434\u0435\u043D\u044C
    \u0432\u0441\u0435\u0433\u0434\u0430 \u0441\u043C\u0435\u043D\u044F\u0435\u0442\u0441\u044F
    \u043D\u043E\u0447\u044C\u044E\u2026 \u0418 \u0432\u043E\u0442 \u043D\u0430\u0441\u0442\u0430\u043B\u0438
    \u0442\u0435\u043C\u043D\u044B\u0435 \u0432\u0440\u0435\u043C\u0435\u043D\u0430
    \u0434\u043B\u044F \u0410\u043B\u0435\u043D\u0434\u043E\u0440\u0430.\r\n\u0412\u0441\u0435
    \u043D\u0430\u0447\u0430\u043B\u043E\u0441\u044C \u0441 \u0442\u043E\u0433\u043E,
    \u043A\u0430\u043A \u0447\u0443\u0434\u0435\u0441\u043D\u043E\u0435 \u0433\u043E\u043B\u0443\u0431\u043E\u0435
    \u043D\u0435\u0431\u043E \u0437\u0430\u0442\u044F\u043D\u0443\u043B\u043E\u0441\u044C
    \u0447\u0435\u0440\u043D\u044B\u043C \u043A\u0443\u043F\u043E\u043B\u043E\u043C
    \u0441\u0442\u0440\u0430\u0445\u0430. \u0412\u043E\u043B\u0448\u0435\u0431\u043D\u044B\u0435
    \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0430, \u0436\u0438\u0432\u0443\u0449\u0438\u0435
    \u043D\u0430 \u043E\u0433\u0440\u043E\u043C\u043D\u044B\u0445 \u043F\u0440\u043E\u0441\u0442\u043E\u0440\u0430\u0445
    \u0446\u0432\u0435\u0442\u043E\u0447\u043D\u044B\u0445 \u043F\u043E\u043B\u0435\u0439,
    \u043D\u0435\u043E\u0436\u0438\u0434\u0430\u043D\u043D\u043E \u0438\u0441\u0447\u0435\u0437\u043B\u0438,
    \u0430 \u0436\u0438\u0437\u043D\u044C \u0432 \u0446\u0432\u0435\u0442\u0430\u0445
    \u0441\u0442\u0430\u043B\u0430 \u043C\u0435\u0434\u043B\u0435\u043D\u043D\u043E
    \u0443\u0433\u0430\u0441\u0430\u0442\u044C. \u0418 \u043E\u0434\u043D\u0430 \u0442\u0435\u043C\u043D\u0430\u044F-\u0442\u0435\u043C\u043D\u0430\u044F
    \u043D\u043E\u0447\u044C \u0432\u044B\u0447\u0435\u0440\u043A\u043D\u0443\u043B\u0430
    \u0432\u0441\u0435 \u0441\u0430\u043C\u043E\u0435 \u043F\u0440\u0435\u043A\u0440\u0430\u0441\u043D\u043E\u0435
    \u0438 \u0434\u043E\u0431\u0440\u043E\u0435 \u0438\u0437 \u0436\u0438\u0437\u043D\u0438
    \u043C\u0430\u043B\u044C\u0447\u0438\u043A\u0430\u2026\r\n\n1. \u041F\u0440\u043E\u0434\u043E\u043B\u0436\u0438\u0442\u044C"
